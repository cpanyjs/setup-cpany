{
  "name": "setup-cpany",
  "version": "0.0.7",
  "private": true,
  "description": "Setup CPany environment",
  "keywords": [
    "actions",
    "node",
    "setup",
    "CPany"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cpanyjs/setup-cpany.git"
  },
  "license": "MIT",
  "author": "XLor",
  "main": "lib/main.js",
  "scripts": {
    "all": "pnpm run build && pnpm run format && pnpm run lint && pnpm run package",
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "release": "node scripts/release.js"
  },
  "dependencies": {
    "@actions/cache": "^4.0.3",
    "@actions/core": "^1.11.1",
    "@actions/exec": "^1.1.1",
    "global-dirs": "^4.0.0",
    "js-yaml": "^4.1.0",
    "kolorist": "^1.8.0",
    "resolve": "^1.22.10"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.14.1",
    "@types/resolve": "^1.20.6",
    "@typescript-eslint/parser": "^8",
    "@vercel/ncc": "^0.38.3",
    "eslint": "^8.57.1",
    "eslint-plugin-github": "^4.10.2",
    "execa": "^9.5.2",
    "fs-extra": "^11.3.0",
    "prettier": "2.4.1",
    "typescript": "^5.8.3"
  },
  "packageManager": "pnpm@10.8.1",
  "engines": {
    "node": ">=v18.0.0"
  }
}
